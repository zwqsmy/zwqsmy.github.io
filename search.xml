84define(<span class="string">&#x27;ABSPATH&#x27;</span>, <span class="built_in">dirname</span>(__FILE__) . <span class="string">&#x27;/&#x27;</span>);</span><br><span class="line">    85</span><br><span class="line">    86/** 设置WordPress变量和包含文件。 */</span><br><span class="line">    87require_once(ABSPATH . <span class="string">&#x27;wp-settings.php&#x27;</span>);</span><br><span class="line">    88</span><br><span class="line"></span><br><span class="line">C.安装</span><br><span class="line">D.登录</span><br></pre></td></tr></table></figure>]]></content>
      
      
      
